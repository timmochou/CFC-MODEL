</Parameter>
<Parameter>
<Attributes name="P_FILENAME"/>
<O t="XMLable" class="com.fr.base.Formula">
<Attributes>
<![CDATA[=P_FILENAME]]></Attributes>
</O>
</Parameter>
</Parameters>
<Content>
<![CDATA[var iframe = $("<iframe id='inp' name='inp' width='100%' height='100%' scrolling='yes' frameborder='0'>");
var locale = this.options.form.getWidgetByName("fr_locale").getValue()
var P_ID = []A;

//取下拉框參數
var P_CFC = _g().parameterEl.getWidgetByName("P_CFC").getValue()
var P_DATE = _g().parameterEl.getWidgetByName("P_DATE").getValue()
var P_VERSION = _g().parameterEl.getWidgetByName("P_VERSION").getValue()

for (var i = 14; i <= _g().getCellValue("C12"); i++) {
	if (_g().getCellValue("D" + i).length = 0) {
		break
	}
	if (_g().getCellValue("C" + i) === true) {
		P_ID.push(_g().getCellValue("D" + i));
	}
}
//
//if(P_ID.length == 0 && C6 != true){
//	alert("No record selected!")
//	return
//}

var url = "${GP_RELATIVE_LINK}&op=write&ope=A2&fr_locale="+locale+"&P_ID=" + P_ID+"&P_CFC="+P_CFC+"&P_DATE="+P_DATE+"&P_VERSION="+P_VERSION +"&P_FILENAME=" + P_FILENAME;
iframe.attr("src", url);

var o = {
	title: edit,
	width: 1500,
	height: 800
};

FR.showDialog(o.title, o.width, o.height, iframe);
$('.fr-core-window-header').css("background", "#D04A02")
$(".fr-core-panel-tool-close").css("top","5px");
//按鈕close，放css不生效]]></Content>
</JavaScript>
