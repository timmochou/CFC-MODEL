<TableDataMap>
<TableData name="DIC_ENTITY" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="P_DATE"/>
<O>
<![CDATA[2023-12]]></O>
</Parameter>
<Parameter>
<Attributes name="P_VERSION"/>
<O>
<![CDATA[Per Audit]]></O>
</Parameter>
<Parameter>
<Attributes name="fr_locale"/>
<O>
<![CDATA[zh_TW]]></O>
</Parameter>
<Parameter>
<Attributes name="P_CFC"/>
<O>
<![CDATA[P00001_TW]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	--t1.INV_ENTITY_CODE,
	t4.CURRENT_CODE AS INV_ENTITY_CODE,
	t3.COUNTRY_ID,
	t3.ENTITY_NAME,
	t2.IS_LOWTAX
FROM TRS_FACT_GROUP_INV_REL t1
LEFT JOIN V_TRS_DIM_ENTITY_CUR t4 ON t1.INV_ENTITY_CODE = t4.ENTITY_CODE
LEFT JOIN V_TRS_DIM_ENTITY t3 ON t4.CURRENT_CODE = t3.ENTITY_ID
LEFT JOIN TRS_FACT_COUNTRY_TAX t2 ON t3.COUNTRY_ID = t2.COUNTRY_CODE
WHERE 
	t1.ENTITY_CODE = '${P_CFC}'
	AND t3.LANGUAGE = '${fr_locale}'
	AND t1.START_DATE <= EOMONTH(CAST('${P_DATE}' + '-01' AS DATE))
	AND t1.END_DATE >= EOMONTH(CAST('${P_DATE}' + '-01' AS DATE))
	AND COMPANY_ID NOT IN (SELECT CFC_INV_ENTITY_CODE FROM TRS_FACT_CFC_INV_RECOGNIZED_AMOUNT WHERE CFC_ENTITY_CODE = '${P_CFC}' AND FORMAT(PERIOD, 'yyyy-MM') ='${P_DATE}' AND SCENARIO = '${P_VERSION}')
ORDER BY t1.INV_ENTITY_CODE]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="DIC_ENTITY_ALL" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="P_CFC"/>
<O>
<![CDATA[P00019_WS_NEW]]></O>
</Parameter>
<Parameter>
<Attributes name="P_DATE"/>
<O>
<![CDATA[2023-12]]></O>
</Parameter>
<Parameter>
<Attributes name="fr_locale"/>
<O>
<![CDATA[zh_TW]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[--用來自動帶出新增資料的所在國家和所在地區是否為低稅區
SELECT
	--t1.INV_ENTITY_CODE,
	t4.CURRENT_CODE AS INV_ENTITY_CODE,
	t3.COUNTRY_ID,
	t3.ENTITY_NAME,
	t2.IS_LOWTAX
FROM TRS_FACT_GROUP_INV_REL t1
LEFT JOIN V_TRS_DIM_ENTITY_CUR t4 ON t1.INV_ENTITY_CODE = t4.ENTITY_CODE
LEFT JOIN V_TRS_DIM_ENTITY t3 ON t4.CURRENT_CODE = t3.ENTITY_ID
LEFT JOIN TRS_FACT_COUNTRY_TAX t2 ON t3.COUNTRY_ID = t2.COUNTRY_CODE
WHERE 
	t1.START_DATE <= EOMONTH(CAST('${P_DATE}' + '-01' AS DATE))
	AND t1.END_DATE >= EOMONTH(CAST('${P_DATE}' + '-01' AS DATE))
	AND LANGUAGE = '${fr_locale}'
	AND t4.CURRENT_CODE = '${P_CFC}'
ORDER BY t4.CURRENT_CODE]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="REP_TOTAL" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="P_CFC"/>
<O>
<![CDATA[P00013_BVI]]></O>
</Parameter>
<Parameter>
<Attributes name="P_DATE"/>
<O>
<![CDATA[2023-12]]></O>
</Parameter>
<Parameter>
<Attributes name="P_VERSION"/>
<O>
<![CDATA[Per Audit]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	ID,
	t1.PERIOD,
	t2.CURRENT_CODE AS CFC_ENTITY_CODE,
	--t1.CFC_ENTITY_CODE,
	t1.SCENARIO,
	TAX_REV_TOTAL AS TAX_REV_TOTAL_ORI,
	TAX_EXP_TOTAL AS TAX_EXP_TOTAL_ORI,
	RE_ADD_TOTAL AS RE_ADD_TOTAL_ORI,
	RE_LESS_TOTAL AS RE_LESS_TOTAL_ORI,
	t2.CURRENCY_ID,
	AVG_EXCHANGE_RATIO,
	TAX_REV_TOTAL * AVG_EXCHANGE_RATIO AS TAX_REV_TOTAL,
	TAX_EXP_TOTAL * AVG_EXCHANGE_RATIO AS TAX_EXP_TOTAL,
	RE_ADD_TOTAL * AVG_EXCHANGE_RATIO AS RE_ADD_TOTAL,
	RE_LESS_TOTAL * AVG_EXCHANGE_RATIO AS RE_LESS_TOTAL
FROM TRS_FACT_CFC_INV_RECOGNIZED_AMOUNT_TOTAL t1
LEFT JOIN V_TRS_DIM_ENTITY_CUR t2 ON t1.CFC_ENTITY_CODE = t2.ENTITY_CODE
LEFT JOIN V_TRS_FACT_EXCHANGE_BANKOFTW t3 ON t2.CURRENCY_ID = t3.CURRENCY_ID AND FORMAT(t1.PERIOD, 'yyyy-MM') = t3.PERIOD AND t1.SCENARIO = t3.SCENARIO AND TARGET_CURRENCY_ID = 'NTD'
WHERE 1 = 1
	--AND t1.CFC_ENTITY_CODE = '${P_CFC}'
	AND t2.CURRENT_CODE = '${P_CFC}'
	AND FORMAT(t1.PERIOD, 'yyyy-MM') = '${P_DATE}'
	AND t1.SCENARIO = '${P_VERSION}']]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
