<TableData name="REP_INV_RECONIZED_AMOUNT" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="P_CFC"/>
<O>
<![CDATA[P00012_BVI]]></O>
</Parameter>
<Parameter>
<Attributes name="P_DATE"/>
<O>
<![CDATA[2023-12]]></O>
</Parameter>
<Parameter>
<Attributes name="P_VERSION"/>
<O>
<![CDATA[Per Audit]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	t1.PERIOD,
	t1.SCENARIO,
	t1.BASE_ENTITY,
	t1.INV_ENTITY,
	t1.RATIO ,
	t1.ID,
	t1.COUNTRY_ID,
	t1.IS_LOWTAX,
	t1.CURRENCY_ID,
	t1.AVG_EXCHANGE_RATIO,
	t1.AMOUNT,
	t1.RE_AMOUNT,
	t1.CFC_TAX,
	t1.CFC_CURRENCY,
	t1.CFC_EXCHANGE_RATIO
FROM V_TRS_FACT_CFC_INV_RECOGNIZED_AMOUNT_DETAIL t1
--LEFT JOIN V_TRS_DIM_ENTITY t2 ON t1.BASE_ENTITY = t2.ENTITY_CODE AND t2.ENTITY_TYPE_ID = '003'
WHERE 1 = 1
--	AND t2.CURRENT_CODE = '${P_CFC}'
	AND BASE_ENTITY = '${P_CFC}'
	AND FORMAT(t1.PERIOD, 'yyyy-MM') = '${P_DATE}'
	AND t1.SCENARIO = '${P_VERSION}'
--	AND t2.LANGUAGE = '${fr_locale}'
ORDER BY BASE_ENTITY]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
