<![CDATA[A0]]></O>
</Parameter>
<Parameter>
<Attributes name="P_COMPANY"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="P_ID"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="P_PERIOD_START"/>
<O>
<![CDATA[2023-06-01]]></O>
</Parameter>
<Parameter>
<Attributes name="P_PERIOD_END"/>
<O>
<![CDATA[2024-06-29]]></O>
</Parameter>
<Parameter>
<Attributes name="fr_locale"/>
<O>
<![CDATA[zh_TW]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH GRP AS (
SELECT DISTINCT 
'1' AS TPE,
t1.COMPANY_CODE,
t2.COMPANY_NAME
FROM V_TRS_DIM_COMPANY_CUR t1
LEFT JOIN TRS_DIM_COMPANY_I18N t2 ON t1.CURRENT_CODE = t2.COMPANY_ID
WHERE t2.language = '${fr_locale}' AND t1.show='true'
)
, equity AS (
SELECT '3' AS TPE,
ENTITY_CODE AS COMPANY_CODE,
ENTITY_NAME AS COMPANY_NAME
FROM trs_dim_equity_inv_list
)
, V1 AS ( 
SELECT * FROM GRP
UNION
SELECT * FROM equity
)
, Dic_INVed_Company AS (
SELECT * FROM V1
where 1=1
--ORDER BY TPE, COMPANY_NAME
)
, dic_adjitem as (
SELECT 
T1.INV_ADJITEM_CODE,
T1.INV_ADJITEM_NAME,
CONCAT(T1.SERIAL_NO, T1.INV_ADJITEM_CODE) AS CONCAT_KEY
FROM "V_TRS_DIM_INV_ADJITEM" T1
WHERE language='${fr_locale}'
)
, group_nongroup as (
SELECT t1.*
FROM TRS_FACT_GROUP_INV_REL t1
--where t1.TYPE = 'group' AND t3.language='${fr_locale}' AND t2.show = 'true'
)
select distinct 
t.* ,
j.INV_ADJITEM_NAME
from group_nongroup t
left join dic_adjitem j on t.INV_ADJITEM_CODE = j.INV_ADJITEM_CODE
where 1=1
${if(ope = 'A1', "AND 1 = 0", "")}
${if(len(P_COMPANY)=0,"","AND t.ENTITY_code IN ('"+P_COMPANY+"')")}
--${if(len(P_INV_TYPE)=0,"","AND t.TYPE IN ('"+P_INV_TYPE+"')")}
${if(ope="A2","AND t.ID IN ('"+replace(P_ID,",","','")+"')","")}
${if(ope = 'A0',"AND END_DATE >= CONVERT(DATE,'"+P_PERIOD_START+"', 23) AND START_DATE <= CONVERT(DATE,'"+P_PERIOD_END+"', 23)","")}
order by t.type, t.ENTITY_code, t.INV_entity_CODE, t.start_date, t.end_date]]></Query>
