<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH 
V1 AS (
    SELECT DISTINCT 
    	   '2' as TPE,
        t1.INV_ENTITY_CODE  AS ENTITY_CODE,
        t1.INV_ENTITY_CODE  AS ENTITY_NAME,
        'non-group' AS INV_TYPE
    FROM TRS_FACT_REL_PARTY_INFO t1
    LEFT JOIN V_TRS_DIM_ENTITY t2 ON t1.INV_ENTITY_CODE = t2.entity_CODE 
    WHERE t2.FR_LOCALE IS NULL  
),
v2 AS (
    SELECT DISTINCT 
    '1' as TPE,
        t1.ENTITY_CODE AS ENTITY_CODE, 
        t2.company_name AS ENTITY_NAME,
        'group' AS INV_TYPE
    FROM V_TRS_DIM_ENTITY t1
    LEFT JOIN V_TRS_DIM_COMPANY t2 ON t1.COMPANY_ID = t2.COMPANY_ID AND t1."LANGUAGE" = t2."LANGUAGE" AND t2.IS_ACTIVE = 'true'
    --JOIN V_TRS_DATA_AUTHORIZATION t3 ON t1.entity_code = t3.ent_id 
    WHERE t1.ENTITY_TYPE_ID = '003' AND t1.LANGUAGE = '${fr_locale}' 
    --AND t3.username = '${GP_USERNAME}' AND t3.MODULE = 'App05'
),
v3 AS (
    SELECT 
	   '3' as TPE,
        entity_code,
        entity_name,
        'non-group' AS inv_TYPE
    FROM trs_dim_equity_inv_list
),
v AS (
    SELECT * FROM v1
    UNION
    SELECT * FROM v2
    UNION
    SELECT * FROM v3
)
SELECT * FROM v
--where 1=1 and INV_TYPE = '${E7}'
ORDER BY TPE, INV_TYPE, ENTITY_NAME]]></Query>
