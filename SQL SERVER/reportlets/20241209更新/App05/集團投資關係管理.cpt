<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH TRS_DIM_ENTITY_v AS (
SELECT DISTINCT 
t1.*,
t2.*
FROM v_trs_dim_entity_cur t1
LEFT JOIN trs_dim_entity_i18n t2 ON t1.current_code = t2.entity_id
WHERE t1.SHOW = 'true'
)
, TRS_DIM_COMPANY_V AS (
SELECT DISTINCT 
t1.*,
t2.*
FROM v_trs_dim_company_cur t1
LEFT JOIN trs_dim_company_i18n t2 ON t1.current_code = t2.company_id
WHERE t1.SHOW = 'true'
)
,V1 AS (
SELECT DISTINCT 
    	   '2' as TPE,
        t1.INV_ENTITY_CODE AS ENTITY_CODE,
        t1.INV_ENTITY_CODE  AS ENTITY_NAME,
        'non-group' AS INV_TYPE
    FROM TRS_FACT_REL_PARTY_INFO t1
    LEFT JOIN TRS_DIM_ENTITY_V t2 ON t1.INV_ENTITY_CODE = t2.CURRENT_CODE 
    WHERE t2."LANGUAGE" IS NULL  
)
, v2 AS (
SELECT DISTINCT 
    '1' as TPE,
        t1.ENTITY_CODE AS ENTITY_CODE, 
        t2.company_name AS ENTITY_NAME,
        'group' AS INV_TYPE
    FROM TRS_DIM_ENTITY_V t1
    LEFT JOIN TRS_DIM_COMPANY_V t2 ON t1.CURRENT_CODE = t2.CURRENT_CODE
    WHERE t1.ENTITY_TYPE_ID = '003' AND t2.LANGUAGE = '${fr_locale}' 
),
v3 AS (
    SELECT 
	   '3' as TPE,
        entity_code,
        entity_name,
        'non-group' AS inv_TYPE
    FROM trs_dim_equity_inv_list
),
v AS (
    SELECT * FROM v1
    UNION
    SELECT * FROM v2
    UNION
    SELECT * FROM v3
)
SELECT * FROM v
where 1=1
ORDER BY TPE, INV_TYPE, ENTITY_NAME]]></Query>
