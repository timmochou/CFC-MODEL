<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="GP_USERNAME"/>
<O>
<![CDATA[ACC1]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FINEDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 
 t1.USERNAME,
 t1.REALNAME,
 t1.email,
 t1.mobile,
 t4.NAME as DEPARTMENT, 
 t3.NAME as POST,
 t6.name
-- t1.enable#
FROM fine_user t1
LEFT JOIN fine_user_role_middle t2 ON t1.ID=t2.USERID
LEFT JOIN fine_dep_role t5   ON t2.ROLEID=t5.ID
LEFT JOIN fine_post t3    ON t3.ID=t5.POSTID
LEFT JOIN fine_department t4   ON  t4.ID=t5.DEPARTMENTID
LEFT JOIN FINE_CUSTOM_ROLE t6 ON t6.ID=t2.roleID
WHERE username = '${GP_USERNAME}']]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FINEDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH DEPARTMENT AS(
SELECT
t1.id ,
t2.name AS company_display,
t1.name AS entity_display
FROM fine_department t1
JOIN fine_department t2 ON t1.parentid = t2.id
)
SELECT 
t1.username,
t1.realname,
t5.company_display,
t7.company_id,
t5.entity_display,
t6.entity_id,
t4.name AS position_display
FROM fine_user t1
LEFT JOIN fine_user_role_middle t2 ON t1.id = t2.userid
LEFT JOIN fine_dep_role t3 ON t2.roleid = t3.id
LEFT JOIN fine_post t4 ON t3.postid = t4.id
LEFT JOIN DEPARTMENT t5 ON t3.departmentid = t5.id
LEFT JOIN TRSDB.trs_dim_entity t6 ON t6.entity_display = t5.entity_display
LEFT JOIN TRSDB.trs_dim_company t7 ON t7.company_display = t5.company_display
WHERE t2.roletype=1]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
