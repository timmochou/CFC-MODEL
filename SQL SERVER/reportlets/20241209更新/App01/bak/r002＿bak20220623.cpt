<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 
	t.ENTITY_ID ,
	t.COMPANY_ID ,
	t.ENTITY_CODE ,
	t.entity_display,
	t.COUNTRY_ID ,
	t.REGION_ID ,
	t.CURRENCY_ID ,
	t.SALES_FUNCTION_ID ,
	t.PLI ,
	t.IS_ACTIVE ,
	t.ENTITY_TYPE_ID ,
	t.ENTITY_NAME ,
	t.CREATED_BY ,
	t.CREATED_TIME ,
	t.UPDATED_BY ,
	t.UPDATED_TIME ,
	t.ID ,
	t."LANGUAGE" ,
	t.FR_LOCALE 
FROM  V_TRS_DIM_ENTITY t 
WHERE t.FR_LOCALE='${fr_locale}'
AND ENTITY_TYPE_ID IN ('001','002')
${if(LEN(P_COMPANY)=0,"","AND t.ENTITY_ID ='"+P_COMPANY+"'")}
${if(ope="A1","AND t.LANGUAGE='' ","")}
--upload 選出空資料
${if(ope="A2","AND t.ENTITY_ID IN ('"+replace(P_ID,",","','")+"')","")}
ORDER BY 
t.company_id,
t.entity_type_id]]></Query>
