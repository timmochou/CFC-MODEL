<TableData name="Rep_TB_Actual" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="ope"/>
<O>
<![CDATA[A0]]></O>
</Parameter>
<Parameter>
<Attributes name="GP_USERNAME"/>
<O>
<![CDATA[STAFF1]]></O>
</Parameter>
<Parameter>
<Attributes name="P_ENTITY"/>
<O>
<![CDATA[P00018_CN_NEW]]></O>
</Parameter>
<Parameter>
<Attributes name="P_SCENARIO"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="fr_locale"/>
<O>
<![CDATA[zh_TW]]></O>
</Parameter>
<Parameter>
<Attributes name="P_ACCOUNT"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="P_ID"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="P_PERIOD_START"/>
<O>
<![CDATA[2020-11]]></O>
</Parameter>
<Parameter>
<Attributes name="P_PERIOD_END"/>
<O>
<![CDATA[2023-12]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH auth AS (
SELECT DISTINCT 
t1.USERNAME,
t1.REALNAME ,
t1.COMPANY_DISPLAY,
t1.COMP_ID,
t1.ENTITY_DISPLAY,
t1.ENT_ID,
t1.MODULE 
FROM V_TRS_DATA_AUTHORIZATION t1
)

SELECT 
    --count(*)
    t1.ID,
    t1.period,
    t1.month,
    CONVERT(DATE, CONCAT(t1.period, RIGHT('00' + CAST(t1.MONTH AS VARCHAR(2)), 2), '01'), 112) AS DAY,
    CONCAT(t1.period, '-', RIGHT('00' + CAST(t1.MONTH AS VARCHAR(2)), 2)) as PERIOD_filter,
    t3.CURRENT_CODE,
    t1.entity_code,
    t4.entity_name,
    t1.ACCOUNT_CODE,
    t2.account_name,
    t1.ACCUMULATED_AMOUNT,
    t1.CURRENT_AMOUNT,
    t1.CURRENCY_CODE,
    t1.SCENARIO,
    t1.ori_account_code,
    t1.IFRS_VERSION,
    t2.DEBIT_CREDIT,
    CASE WHEN t1.updated_time IS NULL THEN t1.created_time
         ELSE t1.updated_time 
    END AS UPDATE_TIME
FROM TRS_FACT_TRIAL_BALANCE t1
LEFT JOIN V_TRS_DIM_ACCOUNT t2 ON t1.ACCOUNT_CODE = t2.ACCOUNT_CODE AND t1.IFRS_VERSION = t2.IFRS_VERSION AND t2.language = '${fr_locale}'   
LEFT JOIN v_trs_dim_entity_cur t3 ON t1.entity_code = t3.entity_code 
LEFT JOIN TRS_DIM_ENTITY_I18N t4 ON t3.CURRENT_CODE = t4.ENTITY_ID 
JOIN AUTH t5 ON t3.CURRENT_CODE = t5.ENT_ID
WHERE 1=1 
    AND t5.MODULE = 'App05'
    AND t4.language = '${fr_locale}' 
    AND t5.USERNAME = '${GP_USERNAME}' 
    ${IF(ope = 'A1', "AND 1 = 0", "")} --註解1：在新增視窗顯示欄位
    ${if(ope="A2","AND t1.ID IN ('"+replace(P_ID,",","','")+"')","")} --註解2:在查看視窗篩選要編輯的資料進入編輯視窗
    ${if(ope="A0","AND CONCAT(t1.period, '-', RIGHT('0' + CAST(t1.MONTH AS VARCHAR(2)), 2))
  BETWEEN '"+P_PERIOD_START+"' AND '"+P_PERIOD_END+"'","")}
    ${if(LEN(P_SCENARIO)=0,"","AND t1.SCENARIO IN ('"+P_SCENARIO+"')","")}
    ${if(LEN(P_ENTITY)=0,"","AND t3.CURRENT_CODE IN ('"+P_ENTITY+"')","")} 
    ${if(LEN(P_ACCOUNT)=0,"","AND t1.ACCOUNT_CODE IN ('"+P_ACCOUNT+"')","")} 
ORDER BY  t1.PERIOD, t1.month,t1.SCENARIO,t4.entity_name,t1.ACCOUNT_CODE]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="Dic_Account" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="fr_locale"/>
<O>
<![CDATA[zh_TW]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH A AS (
SELECT DISTINCT T1.PARENT_CODE
FROM V_TRS_DIM_ACCOUNT T1
)
SELECT distinct T1.ACCOUNT_CODE,T1.ACCOUNT_NAME,T1.DEBIT_CREDIT
FROM V_TRS_DIM_ACCOUNT T1
LEFT JOIN A T2 ON T1.ACCOUNT_CODE = T2.PARENT_CODE
WHERE T2.PARENT_CODE IS NULL AND t1.language = '${fr_locale}'
order by t1.ACCOUNT_CODE]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="CHECK_KEY" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select concat('A',
     PERIOD+
     MONTH+
     ENTITY_CODE+
     ACCOUNT_CODE+
     IFRS_VERSION+
     ORI_ACCOUNT_CODE+
     SCENARIO) AS CHECK_KEY
from TRS_FACT_TRIAL_BALANCE]]></Query>
