var l_account = this.options.form.getWidgetByName("LAB_ACCOUNT");
var l_scenario = this.options.form.getWidgetByName("LAB_SCENARIO");
var scenario = this.options.form.getWidgetByName("P_SCENARIO");



if(ope==="A0"){
	search.setVisible(true);
	upload_data.setVisible(true);
	edit_record.setVisible(true);
	delete_record.setVisible(true);
	start.setVisible(true);
	end.setVisible(true);
	lab_period.setVisible(true);
	lab_period2.setVisible(true);
     company.setVisible(true);
	company_l.setVisible(true);
	l_account.setVisible(true);
	account.setVisible(true);
     l_scenario.setVisible(true);
     scenario.setVisible(true);

}else if(ope==="A1"){
	submit.setVisible(true);
	import_data.setVisible(true);
}else if(ope==="A2"){
