<![CDATA[2020-07]]></O>
</Parameter>
<Parameter>
<Attributes name="P_COMPANY"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="GP_USERNAME"/>
<O>
<![CDATA[STAFF2]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH CTE AS (
	SELECT
		'CFC_EXEMPTION' AS PREPARATION_TYPE
	UNION ALL
	SELECT
		'CFC_CALCULATION' AS PREPARATION_TYPE
	UNION ALL
	SELECT
		'CFC_INV_INCOME' AS PREPARATION_TYPE
	), CTE2 AS (
SELECT 
t1.PERIOD,
t2.CURRENT_CODE AS ENTITY_CODE,
t1.SCENARIO,
t2.COUNTRY_ID,
CTE.PREPARATION_TYPE,
t1.CREATED_TIME,
t2.CURRENT_CODE
FROM V_TRS_FACT_CFC_ENTITY_VERSION t1
CROSS JOIN CTE
LEFT JOIN V_TRS_DIM_ENTITY_CUR t2 ON t1.ENTITY_CODE = t2.ENTITY_CODE --AND t2.ENTITY_TYPE_ID = '003' AND t2.IS_ACTIVE = 'true'
WHERE
--	LANGUAGE = '${fr_locale}'
	t2.COUNTRY_ID = 'TW'
	${IF(LEN(P_VERSION) = 0, "", "AND SCENARIO IN ('" + P_VERSION + "')")}
	),CTE3 AS (
SELECT
	CTE2.PERIOD,
	CTE2.ENTITY_CODE,
	CTE2.SCENARIO,
	CTE2.COUNTRY_ID,
	CTE2.PREPARATION_TYPE,
	COALESCE(t2.STATUS , 'false') AS STATUS,
	CTE2.CREATED_TIME,
	CTE2.CURRENT_CODE
FROM CTE2
LEFT JOIN TRS_FACT_CFC_DECLARATION t2 ON CTE2.ENTITY_CODE = t2.ENTITY_CODE AND CTE2.PREPARATION_TYPE = t2.PREPARATION_TYPE AND CTE2.PERIOD = t2.PERIOD AND CTE2.SCENARIO = t2.SCENARIO
--ORDER BY ENTITY_CODE
)
SELECT 
	*
FROM
(
	SELECT 
		PERIOD,
		ENTITY_CODE,
		SCENARIO,
		COUNTRY_ID,
		PREPARATION_TYPE,
		STATUS,
		CREATED_TIME,
		CURRENT_CODE
	FROM CTE3
	) AS T3
	PIVOT (
	MAX(STATUS) --AS STATUS
	FOR PREPARATION_TYPE IN([CFC_EXEMPTION]A ,[CFC_CALCULATION]A ,[CFC_INV_INCOME]A)
) t1
JOIN V_TRS_DATA_AUTHORIZATION t3 ON t1.ENTITY_CODE = t3.ENT_ID AND USERNAME = '${GP_USERNAME}' AND MODULE = 'App05'
WHERE 
	FORMAT(PERIOD,'yyyy-MM') BETWEEN '${P_START_DATE}' AND '${P_END_DATE}'
	${IF(LEN(P_COMPANY) = 0, "", "AND CURRENT_CODE IN ('" +P_COMPANY + "')")}
	${IF(LEN(P_VERSION) = 0, "", "AND SCENARIO IN ('" + P_VERSION + "')")}
ORDER BY PERIOD]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
