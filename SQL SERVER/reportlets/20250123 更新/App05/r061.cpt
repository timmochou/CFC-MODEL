<Query>
<![CDATA[SELECT
	DISTINCT
	t1.PERIOD,
	t1.ENTITY_CODE,
	t1.SCENARIO,
	t2.COUNTRY_ID,
	t4.ENTITY_NAME--,
--	t2.CURRENT_CODE
FROM V_TRS_FACT_CFC_ENTITY_VERSION t1
LEFT JOIN V_TRS_DIM_ENTITY_CUR t2 ON t1.ENTITY_CODE = t2.ENTITY_CODE 
JOIN V_TRS_DATA_AUTHORIZATION t3 ON t1.ENTITY_CODE = t3.ENT_ID AND USERNAME = '${GP_USERNAME}' AND MODULE = 'App05'
LEFT JOIN TRS_DIM_ENTITY_I18N t4 ON t1.ENTITY_CODE = t4.ENTITY_ID AND t4.LANGUAGE = '${fr_locale}'
WHERE 1 = 1
	AND t2.COUNTRY_ID = 'TW'
	AND t2.SHOW = 'true']]></Query>
