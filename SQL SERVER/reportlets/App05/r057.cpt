</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT DISTINCT
t1.ENTITY_CODE,
t2.CURRENT_CODE,
t3.COMPANY_NAME
FROM V_TRS_FACT_INV_DETAIL t1
LEFT JOIN V_TRS_DIM_ENTITY t2 ON t1.ENTITY_CODE = t2.ENTITY_CODE
LEFT JOIN V_TRS_DIM_COMPANY t3 ON t2.COMPANY_ID = t3.COMPANY_ID AND t2.LANGUAGE = t3.LANGUAGE
-- 取權限表交集
JOIN v_trs_data_authorization t4 ON  t1.entity_code = t4.ent_id AND username = '${GP_USERNAME}' AND module = 'App05'
WHERE 1=1
AND t2.LANGUAGE='${fr_locale}'
ORDER BY t1.ENTITY_CODE]]></Query>
