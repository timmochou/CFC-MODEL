<JavaScript class="com.fr.js.JavaScriptImpl">
<Parameters>
<Parameter>
<Attributes name="P_TITLE"/>
<O t="XMLable" class="com.fr.base.Formula">
<Attributes>
<![CDATA[=i18n("edit_record")]]></Attributes>
</O>
</Parameter>
</Parameters>
<Content>
<![CDATA[var iframe = $("<iframe id='inp' name='inp' width='100%' height='100%' scrolling-'yes' frameborder='0'>");
var locale = this.options.form.getWidgetByName("fr_locale").getValue()
var P_ID = []A;

//取下拉框參數
var P_COMPANY = _g().parameterEl.getWidgetByName("P_COMPANY").getValue()
var P_SECURITY = _g().parameterEl.getWidgetByName("P_SECURITY").getValue()
var P_ACCOUNT = _g().parameterEl.getWidgetByName("P_ACCOUNT").getValue()

for (var i = 7; i <= contentPane.curLGP.getCellValue("C4"); i++) {
	if (contentPane.curLGP.getCellValue("D" + i).length = 0) {
		
		break
		
	}
	if (contentPane.curLGP.getCellValue("C" + i) === true) {
		P_ID.push(contentPane.curLGP.getCellValue("D" + i));
	}
}

if(P_ID.length == 0){
	alert("No record selected!")
	return
}

var url = "${servletURL}?viewlet=App05%252Fr057.cpt&op=write&ope=A2&fr_locale="+locale+"&P_ID=" + P_ID+"&P_COMPANY="+P_COMPANY+"&P_ACCOUNT="+P_ACCOUNT+"&P_SECURITY="+P_SECURITY;
iframe.attr("src", url);
console.log(P_ACCOUNT)
console.log(P_COMPANY)
console.log(P_SECURITY)
var o = {
	title: P_TITLE,
	width: 1350,
	height: 600
};

FR.showDialog(o.title, o.width, o.height, iframe, o);
$('.fr-core-window-header').css("background", "#D04A02")
$(".fr-core-panel-tool-close").css("top","5px");
//按鈕close，放css不生效]]></Content>
</JavaScript>
