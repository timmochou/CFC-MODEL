<Content>
<![CDATA[var search = this.options.form.getWidgetByName("BTN_SEARCH");
var edit_record = this.options.form.getWidgetByName("BTN_EDIT_RECORD");
var upload_data = this.options.form.getWidgetByName("BTN_UPLOAD_DATA");
var delete_record = this.options.form.getWidgetByName("BTN_DELETE_RECORD");
var submit = this.options.form.getWidgetByName("BTN_SUBMIT");
var import_data = this.options.form.getWidgetByName("BTN_IMPORT");
var batch_upload = this.options.form.getWidgetByName("BTN_BATCH_UPLOAD");
var edit = this.options.form.getWidgetByName("BTN_EDIT_RECORD");
var company = this.options.form.getWidgetByName("P_COMPANY");
var company_l = this.options.form.getWidgetByName("LABEL_P_COMPANY");
var security = this.options.form.getWidgetByName("P_SECURITY");
var security_l = this.options.form.getWidgetByName("LABEL_P_SECURITY");
var account = this.options.form.getWidgetByName("P_ACCOUNT");
var account_l = this.options.form.getWidgetByName("LABEL_P_ACCOUNT");
var start_date = this.options.form.getWidgetByName("P_START_DATE");
var end_date = this.options.form.getWidgetByName("P_END_DATE");
var interval_l = this.options.form.getWidgetByName("LABEL_INTERVAL");
var interval_l2 = this.options.form.getWidgetByName("LABEL_INTERVAL2");

if (ope === "A0") {
	search.setVisible(true);
	upload_data.setVisible(true);
	edit_record.setVisible(true);
	delete_record.setVisible(true);
	batch_upload.setVisible(true);
	edit.setVisible(true);
	//下拉框標籤開啟
	company.setVisible(true);
	company_l.setVisible(true);
	security.setVisible(true);
	security_l.setVisible(true);
	account.setVisible(true);
	account_l.setVisible(true);
	interval_l.setVisible(true);
	interval_l2.setVisible(true);
	start_date.setVisible(true);
	end_date.setVisible(true);
} else if (ope === "A1") {
	setTimeout(function() {
		_g().parameterCommit();
	}, 700)
	submit.setVisible(true);
	import_data.setVisible(true);
} else if (ope === "A2") {
	setTimeout(function() {
		_g().parameterCommit();
	}, 700)
	submit.setVisible(true);
	delete_record.setVisible(true);
}]]></Content>
</JavaScript>
