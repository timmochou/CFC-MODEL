<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH FILES AS (
SELECT 
t1.TRS_FACT_INV_ID,
t1.ATTACHMENT
FROM TRS_FACT_INV_DETAIL t1
JOIN V_TRS_DIM_ENTITY_CUR t2 ON t1.ENTITY_CODE = t2.ENTITY_CODE
WHERE 1=1 
${if(LEN(P_COMPANY)=0,"","AND CURRENT_CODE IN ('"+P_COMPANY+"')")}
${if(LEN(P_SECURITY)=0,"","AND INV_SECURITY_CODE IN ('"+P_SECURITY+"')")}
${if(LEN(P_ACCOUNT)=0,"","AND INV_ACCOUNT_CODE IN ('"+P_ACCOUNT+"')")}
${if(ope="A0","AND PERIOD BETWEEN TO_DATE('"+P_START_DATE+"','yyyy-MM-dd') AND TO_DATE('"+P_END_DATE+"','yyyy-MM-dd')","")}
--批量匯入時條件
${if(ope="A1","AND 1=0 ","")}
--批量編輯時條件
${if(ope="A2","AND TRS_FACT_INV_ID IN ('"+replace(P_ID,",","','")+"')","")}
)
SELECT 
  t1.TRS_FACT_INV_ID,
  t1.ENTITY_CODE,
  t1.INV_SECURITY_CODE,
  t1.INV_CATEGORY_CODE,
  t1.INV_ACCOUNT_CODE, 
  t1.AREA,
  t1.ADD_LESS,
  NVL(t1.SHARE_QTY,0) AS SHARE_QTY,
  t1.ACC_SHARE_QTY,
  t1.COST_NTD_USER,
  t1.COST_NTD_SYS,
  t1.COST_NTD,
  t1.ACC_COST_NTD,
  t1.PERIOD,
  t1.SERIAL,
  t1.INV_ADJITEM_CODE,
  t1.ITEM_NOTE,
  t1.CURRENCY_CODE,
  t1.COST_ORIGIN_CURR,
  t1.SALE_PRICE_NTD,
  t1.TRADE_TAX,
  t1.TRADE_FEE,
  t1.NOTE,
  t2.ATTACHMENT,
  t1.FILENAME
FROM V_TRS_FACT_INV_DETAIL t1
JOIN FILES t2 ON t1.TRS_FACT_INV_ID = t2.TRS_FACT_INV_ID
JOIN v_trs_data_authorization t3 ON  t1.entity_code = t3.ent_id AND username = '${GP_USERNAME}' AND module = 'App05'
WHERE 1=1
${if(LEN(P_COMPANY)=0,"","AND t1.ENTITY_CODE IN ('"+P_COMPANY+"')")}
${if(LEN(P_SECURITY)=0,"","AND t1.INV_SECURITY_CODE IN ('"+P_SECURITY+"')")}
${if(LEN(P_ACCOUNT)=0,"","AND t1.INV_ACCOUNT_CODE IN ('"+P_ACCOUNT+"')")}
${if(ope="A0","AND PERIOD BETWEEN TO_DATE('"+P_START_DATE+"','yyyy-MM-dd') AND TO_DATE('"+P_END_DATE+"','yyyy-MM-dd')","")}
--批量匯入時條件
${if(ope="A1","AND 1=0 ","")}
--批量編輯時條件
${if(ope="A2","AND t1.TRS_FACT_INV_ID IN ('"+replace(P_ID,",","','")+"')","")}
ORDER BY ENTITY_CODE, INV_SECURITY_CODE, INV_CATEGORY_CODE, INV_ACCOUNT_CODE, orders DESC]]></Query>
