<Query>
<![CDATA[SELECT DISTINCT
t1.ENTITY_CODE,
t1.SHOW,
t2.ENTITY_NAME AS COMPANY_NAME
FROM V_TRS_DIM_ENTITY_CUR t1 
LEFT JOIN TRS_DIM_ENTITY_I18N t2 ON t1.ENTITY_CODE = t2.ENTITY_ID
-- 取權限表交集
JOIN v_trs_data_authorization t3 ON  t1.entity_code = t3.ent_id AND username = '${GP_USERNAME}' AND module = 'App05'
WHERE 1=1
AND t2.LANGUAGE='${fr_locale}'
AND t1.ENTITY_TYPE_ID = '003'
AND t1.SHOW= 'true'
ORDER BY t1.SHOW DESC,t1.ENTITY_CODE]]></Query>
