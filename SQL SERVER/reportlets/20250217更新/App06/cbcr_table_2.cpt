<Query>
<![CDATA[SELECT DISTINCT
t1.period,
t1.entity_id,
t1.value,
CASE WHEN data_name != 'col_main_ope_ther_ifmn' THEN  REPLACE(t1.value,'.00','')
ELSE t1.value END AS value,
t1.data_name 
FROM TRS_FACT_COUNTRY_REPORT t1
WHERE 1=1
${if(ope="A1","AND id = ''","")}
${if(LEN(P_ENTITY)=0,"","AND entity_id IN ('"+P_ENTITY+"')")}
${if(LEN(P_PERIOD)=0,"","AND period IN ('"+P_PERIOD+"')")}]]></Query>
