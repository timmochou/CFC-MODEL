<TableData name="Rep_TB_Actual" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="P_MONTH"/>
<O>
<![CDATA[06]]></O>
</Parameter>
<Parameter>
<Attributes name="P_PERIOD"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="ope"/>
<O>
<![CDATA[A0]]></O>
</Parameter>
<Parameter>
<Attributes name="GP_USERNAME"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="P_ENTITY"/>
<O>
<![CDATA[USCXM_2]]></O>
</Parameter>
<Parameter>
<Attributes name="P_SCENARIO"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="fr_locale"/>
<O>
<![CDATA[zh_TW]]></O>
</Parameter>
<Parameter>
<Attributes name="P_ACCOUNT"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="P_ID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH ACCU AS(
SELECT 
t1.ID,
t1.period,
LPAD(t1.MONTH,2,'0') AS MONTH,
t5.CURRENT_CODE AS entity_code,
t3.entity_name,
t1.ACCOUNT_CODE ,
t2.account_name,
CASE WHEN t2.DEBIT_CREDIT ='C' THEN t1.ACCUMULATED_AMOUNT * (-1)
	 WHEN t2.DEBIT_CREDIT ='D' THEN t1.ACCUMULATED_AMOUNT 
	 END AS ACCUMULATED_AMOUNT ,
--t1.ACCUMULATED_AMOUNT ,
CASE WHEN t2.DEBIT_CREDIT ='C' THEN t1.CURRENT_AMOUNT * (-1)
	 WHEN t2.DEBIT_CREDIT ='D' THEN t1.CURRENT_AMOUNT
	 END AS CURRENT_AMOUNT ,
--t1.CURRENT_AMOUNT ,
t1.CURRENCY_CODE,
t1.SCENARIO,
t1.ori_account_code,
t2.DEBIT_CREDIT ,
t2.DEBIT_CREDIT AS C_D,
CASE WHEN t1.updated_time IS NULL THEN t1.created_time
ELSE t1.updated_time END AS UPDATE_TIME
FROM TRS_FACT_TRIAL_BALANCE t1
LEFT JOIN V_TRS_DIM_ACCOUNT t2 ON t1.ACCOUNT_CODE =t2.ACCOUNT_CODE  AND t1.IFRS_VERSION = t2.IFRS_VERSION 
LEFT JOIN V_TRS_DIM_ENTITY_CUR t5 ON t1.entity_code = t5.entity_code
LEFT JOIN v_trs_dim_entity t3 ON t5.current_code = t3.entity_code AND t2.language=t3.language
JOIN v_trs_data_authorization t4 ON t5.current_CODE = t4.ent_id -- 取權限表交集 
WHERE 1=1
AND t4.module = 'App01' --'APP03'
AND USERNAME = '${GP_USERNAME}'
AND VALUE_TYPE ='actual'
AND t2.language = '${fr_locale}'
${if(len(P_ENTITY)!=0 && ope =="A0","AND t5.CURRENT_CODE IN ('"+P_ENTITY+"')","")}
${if(len(P_PERIOD)!=0 && ope =="A0","AND t1.PERIOD IN ('"+P_PERIOD+"')","")}
${if(len(P_MONTH)!=0 && ope =="A0","AND LPAD(t1.MONTH,2,'0') IN ('"+P_MONTH+"')","")}
${if(len(P_ACCOUNT)!=0 && ope =="A0","AND t1.ACCOUNT_CODE IN ('"+P_ACCOUNT+"')","")}
${if(len(P_SCENARIO)!=0 && ope =="A0","AND t1.SCENARIO IN ('"+P_SCENARIO+"')","")}
${if(ope="A2","AND t1.ID IN ('"+replace(P_ID,",","','")+"')","")}

)
SELECT * FROM ACCU
PIVOT (
SUM(ACCUMULATED_AMOUNT) AS accu,
sum(current_amount) AS curr
FOR DEBIT_CREDIT 
IN ('C' AS Credit,'D' AS Debit)
	)
ORDER BY SCENARIO, PERIOD DESC ,TO_NUMBER(MONTH,'99') ,ENTITY_CODE ,ACCOUNT_CODE]]></Query>
