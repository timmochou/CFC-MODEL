<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 
	CURRENCY_FACT_ID ,
	CURRENCY_ID ,
	TARGET_CURRENCY_ID ,
	exchange_ratio,
	PERIOD 
FROM TRS_FACT_CURRENCY t
WHERE 1=1
${if(ope="A0","AND t.PERIOD BETWEEN '"+P_PERIOD_START+"' AND '"+P_PERIOD_END+"'","")}

${if(LEN(P_CURRENCY)=0,"","AND t.CURRENCY_ID ='"+P_CURRENCY+"'")}
${if(LEN(P_TARGET)=0,"","AND t.TARGET_CURRENCY_ID ='"+P_TARGET+"'")}
--upload 選出空資料
${if(ope="A1","AND t.CURRENCY_FACT_ID='' ","")}
${if(ope="A2","AND t.CURRENCY_FACT_ID IN ('"+replace(P_ID,",","','")+"')","")}

order by PERIOD,CURRENCY_ID]]></Query>
