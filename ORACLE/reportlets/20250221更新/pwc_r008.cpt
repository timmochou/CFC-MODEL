<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT t1.ENTITY_code,
     t1.COUNTRY_ID,
	t2.LOCAL_CURRENCY_ID,
	t3.AVG_EXCHANGE_RATIO 
	from V_TRS_DIM_ENTITY t1
left join V_TRS_DIM_COMPANY t2 on t1.COMPANY_ID = t2.COMPANY_CODE AND t2.LANGUAGE = '${fr_locale}'
left join V_TRS_FACT_EXCHANGE_BANKOFTW t3 on t3.currency_ID = t2.LOCAL_CURRENCY_ID and t3.scenario = '${P_VERSION}' and t3.period = '${P_DATE}'
AND t3.CURRENCY_ID != t3.TARGET_CURRENCY_ID 
where t1.ENTITY_CODE = '${P_CFC}' and t1.LANGUAGE = '${fr_locale}' and t1.ENTITY_TYPE_ID = '003' ]]></Query>
