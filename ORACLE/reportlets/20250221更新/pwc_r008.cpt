<TableData name="DIC_RATIO" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH CTE AS (
	SELECT
		ENTITY_CODE,
		INV_ENTITY_CODE,
		RATIO_HOLD
	FROM TRS_FACT_GROUP_INV_REL
	WHERE START_DATE = TO_DATE('2022-02-01', 'YYYY-MM-DD') AND END_DATE = TO_DATE('2099-12-31', 'YYYY-MM-DD') 
)
, stock_ratio (base_entity, inv_entity, ratio, path, depth) AS (

  SELECT ENTITY_CODE, INV_ENTITY_CODE, RATIO_HOLD, ENTITY_CODE || '->' || INV_ENTITY_CODE, 1
  FROM CTE
  UNION ALL

  SELECT t1.base_entity, t2.INV_ENTITY_CODE, t1.ratio * t2.RATIO_HOLD, 
         t1.path || '->' || t2.INV_ENTITY_CODE, t1.depth + 1
  FROM stock_ratio t1
  JOIN CTE t2 ON t1.inv_entity = t2.ENTITY_CODE
  WHERE t1.depth < 20 -- 限制遞歸深度以防止無限循環
)
, CTE2 AS (
SELECT * FROM stock_ratio
)
SELECT
	BASE_ENTITY,
	INV_ENTITY,
	SUM(RATIO) AS RATIO
FROM CTE2
GROUP BY BASE_ENTITY, INV_ENTITY
ORDER BY BASE_ENTITY]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
