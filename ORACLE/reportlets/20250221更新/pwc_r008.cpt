</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH CTE AS (
SELECT
	COMPANY_CODE
FROM TRS_DIM_COMPANY
WHERE IS_ACTIVE='true'
UNION ALL 
SELECT
	ENTITY_CODE
FROM TRS_DIM_EQUITY_INV_LIST
ORDER BY COMPANY_CODE)
SELECT
	t1.COMPANY_CODE,
	t3.COMPANY_NAME
FROM CTE t1
LEFT JOIN V_TRS_DIM_COMPANY_CUR t2 on t1.COMPANY_CODE = t2.COMPANY_CODE
LEFT JOIN TRS_DIM_COMPANY_I18N t3 ON t2.CURRENT_CODE = t3.COMPANY_ID
WHERE t3.LANGUAGE = '${fr_locale}' AND t2.SHOW = 'true'
order by t3.COMPANY_NAME]]></Query>
