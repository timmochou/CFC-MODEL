if (ope == 'A0') {
	upload_rec.setVisible(true);
	edit_rec.setVisible(true);
	export_excel.setVisible(true);
	delete_rec.setVisible(true);
	}
	else if (ope === "A1") {
	setTimeout(function() {
		_g().parameterCommit();
	}, 700)
	import_excel.setVisible(true)
	submit.setVisible(true);
} else if (ope === "A2") {
	setTimeout(function() {
		_g().parameterCommit();
	}, 700)
	submit.setVisible(true);
//	delete_rec.setVisible(true);
}
if (P_LOCK == 'true') {
	upload_rec.setEnable(false);
	edit_rec.setEnable(false);
	delete_rec.setEnable(false);
	}
let reader = this.options.form.getWidgetByName("P_READER").getValue();
if (reader != '0') {
	upload_rec.setEnable(false);
	edit_rec.setEnable(false);
	delete_rec.setEnable(false);
}]]></Content>
