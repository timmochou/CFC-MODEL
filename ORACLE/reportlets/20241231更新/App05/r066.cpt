</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT
	t1.ID,
	t1.ENTITY_CODE,
	t1.SCENARIO,
	t1.ITEM_PERIOD,
	t1.ITEM,
	t1.ORIGIN_YEAR,
	t1.AMOUNT_ORI,
	t1.CREATED_TIME,
	t1.CURRENCY_ID,
	t1.AVG_EXCHANGE_RATIO,
	t1.AMOUNT--,
	--t2.CURRENT_CODE
FROM V_TRS_FACT_CFC_RESTRICTED_DISTRIBUTION t1
--LEFT JOIN V_TRS_DIM_ENTITY t2 ON t1.ENTITY_CODE = t2.ENTITY_CODE AND t2.ENTITY_TYPE_ID = '003' AND t2.LANGUAGE = '${fr_locale}'
WHERE 1 = 1
	AND t1.ENTITY_CODE = '${P_CFC}'
	AND TO_CHAR(t1.PERIOD, 'YYYY-MM') = '${P_DATE}'
	AND t1.SCENARIO = '${P_VERSION}'
	${IF(ope = 'A1', "AND 1 = 0", "")}
	${IF(ope = 'A2', "AND t1.ID IN ('" +replace(P_ID, ",", "','") +"')","")}
ORDER BY t1.CREATED_TIME]]></Query>
