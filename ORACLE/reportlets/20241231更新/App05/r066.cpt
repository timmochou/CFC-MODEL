<TableData name="DIC_CHECK" class="com.fr.data.impl.DBTableData">
<Desensitizations desensitizeOpen="false"/>
<Parameters>
<Parameter>
<Attributes name="P_CFC"/>
<O>
<![CDATA[P00019_WS_NEW]]></O>
</Parameter>
<Parameter>
<Attributes name="P_VERSION"/>
<O>
<![CDATA[Per Audit]]></O>
</Parameter>
<Parameter>
<Attributes name="P_DATE"/>
<O>
<![CDATA[2023-12]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[TRSDB]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH T AS(
SELECT 
T1.ID,
CURRENT_CODE AS CFC_ENTITY_CODE,
SCENARIO,
ITEM_PERIOD,
ITEM,
ORIGIN_YEAR,
AMOUNT
FROM TRS_FACT_CFC_RESTRICTED_DISTRIBUTION t1
LEFT JOIN V_TRS_DIM_ENTITY_CUR t2 ON t1.CFC_ENTITY_CODE = t2.ENTITY_CODE
)
SELECT
	t2.CFC_ENTITY_CODE || t2.SCENARIO || TO_CHAR(t2.ITEM_PERIOD, 'yyyy-MM') || t2.ITEM || ORIGIN_YEAR AS CHECK_ARR,
	t2.CFC_ENTITY_CODE || t2.SCENARIO || TO_CHAR(t2.ITEM_PERIOD, 'yyyy-MM') || t2.ITEM || ORIGIN_YEAR || AMOUNT AS CHECK_ARR2,
	T2.ID,
	T2.ID ||t2.CFC_ENTITY_CODE || t2.SCENARIO || TO_CHAR(t2.ITEM_PERIOD, 'yyyy-MM') || t2.ITEM || ORIGIN_YEAR AS CHECK_ARR3
FROM V_TRS_FACT_CFC_ENTITY_VERSION t1
LEFT JOIN T t2 ON t2.CFC_ENTITY_CODE = t1.ENTITY_CODE AND t1.SCENARIO = t2.SCENARIO
	--AND t2.ITEM_PERIOD BETWEEN TRUNC(t1.PERIOD, 'YEAR') AND t1.PERIOD
WHERE 1 = 1
	AND t1.ENTITY_CODE = '${P_CFC}'
	AND t1.SCENARIO = '${P_VERSION}'
	AND TO_CHAR(t1.PERIOD, 'YYYY-MM') = '${P_DATE}']]></Query>
